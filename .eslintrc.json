{
  "extends": [
    "eslint:recommended",
    "plugin:node/recommended"
  ],
  "plugins": [
    "node"
  ],
  "parserOptions": {},
  "env": {
    "node": true
  },
  "globals": {},
  "settings": {
    "node": {
      "allowModules": []
    }
  },
  "rules": {
    "no-console": "off",
    "quotes": [
      "error",
      "single"
    ],
    "no-alert": "error",
    "semi": [
      "error",
      "always"
    ],
    "block-spacing": "error",
    "array-bracket-spacing": [
      "error",
      "never"
    ],
    "brace-style": "error",
    "comma-spacing": [
      "error",
      {
        "before": false,
        "after": true
      }
    ],
    "comma-style": [
      "error",
      "last"
    ],
    "indent": [
      "error",
      2
    ],
    "key-spacing": [
      "error",
      {
        "beforeColon": false,
        "afterColon": true
      }
    ],
    "no-multiple-empty-lines": [
      "error",
      {
        "max": 2
      }
    ],
    "no-spaced-func": "error",
    "no-unneeded-ternary": "error",
    "object-curly-spacing": [
      "error",
      "never"
    ],
    "space-infix-ops": "error",
    "space-unary-ops": "error",
    "no-extra-boolean-cast": "error",
    "block-scoped-var": "error",
    "consistent-return": "error",
    "curly": "error",
    "default-case": "error",
    "eqeqeq": "error",
    "guard-for-in": "error",
    "no-caller": "error",
    "no-else-return": "error",
    "no-labels": "error",
    "no-eval": "error",
    "no-invalid-this": "error",
    "no-multi-spaces": "error",
    "no-redeclare": "error",
    "no-return-assign": "error",
    "no-self-compare": "error",
    "wrap-iife": "error",
    "no-unused-vars": "warn"
  }
}